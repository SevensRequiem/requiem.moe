{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "amphp/amp": "^3.0",
        "appstract/laravel-blade-directives": "^1.14",
        "defuse/php-encryption": "^2.4",
        "doctrine/dbal": "^3.6",
        "fahlisaputra/laravel-minify": "^1.1",
        "guzzlehttp/guzzle": "^7.2",
        "hhxsv5/laravel-s": "^3.7",
        "intervention/image": "^3.3",
        "jakyeru/larascord": "^5.0",
        "james-heinrich/getid3": "^1.9",
        "jorijn/laravel-security-checker": "^2.4",
        "knplabs/github-api": "^3.13",
        "laravel/framework": "^10.10",
        "laravel/octane": "^2.3",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.11",
        "laravel/tinker": "^2.8",
        "matthiasmullie/minify": "^1.3",
        "pusher/pusher-php-server": "^7.2",
        "socialiteproviders/discord": "^4.2",
        "socialiteproviders/github": "^4.1",
        "socialiteproviders/steam": "^4.2",
        "spatie/async": "^1.6",
        "spatie/image-optimizer": "^1.7",
        "spatie/laravel-ciphersweet": "^1.5",
        "spatie/laravel-feed": "^4.3",
        "spatie/laravel-markdown": "^2.3",
        "spatie/laravel-responsecache": "^7.4",
        "spatie/laravel-sitemap": "^7.0",
        "stripe/stripe-php": "^13.12",
        "swooletw/laravel-swoole": "^2.13",
        "symfony/process": "^6.3",
        "torann/geoip": "^3.0",
        "typo3/html-sanitizer": "^2.1",
        "voku/html-min": "^4.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
